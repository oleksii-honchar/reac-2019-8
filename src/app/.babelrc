{
  "presets": [
    "es2015",
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "useBuiltIns": true
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ]
      }
    ],
    "babel-plugin-styled-components",
    [
      "styled-components",
      {
        "minify": true
      }
    ],
    "transform-decorators-legacy",
    "transform-decorators",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-async-to-generator"
  ]
}
