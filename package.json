  "main": "src/index.js",
  "scripts": {
    "prebuild": "yarn clean:dist",
    "build": "yarn build:static",
    "build:static": "bash scripts/build.static.sh",
    "build:ssr": "bash scripts/build.ssr.sh",
    "prebuild:loc": "yarn clean:dist",
    "build:loc": "bash scripts/build.loc.sh",
    "build:loc:static": "bash scripts/build.loc.static.sh",
    "build:loc:ssr": "bash scripts/build.loc.ssr.sh",
    "check-types": "tsc",
    "clean:dist": "rm -rf dist/static",
    "docker:build": "bash ./devops/scripts/build.sh",
    "docker:up": "bash ./devops/scripts/run.sh",
    "docker:up:loc": "bash ./devops/scripts/run.loc.sh",
    "docker:up:loc:isolated": "bash ./devops/scripts/run.loc.isolated.sh",
    "launch:loc": "bash ./scripts/launch.loc.sh",
    "lint": "npx eslint src --ext .js,.jsx,.json --config config/eslint.config.js",
    "lint:fix": "npx eslint src --ext .js,.jsx,.json --fix --config config/eslint.config.js",
    "start": "watch",
    "test": "jest --config ./config/jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "watch": "bash scripts/watch.loc.sh",
    "watch:static": "bash scripts/watch.loc.static.sh",
    "watch:ssr": "bash scripts/watch.loc.ssr.sh"
  },
